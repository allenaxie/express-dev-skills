 <!-- what is displayed on browser -->
<%- include('../partials/header') %>
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Express-dev-skills</title>
</head>
<body>
    <body>
        <h1>Skills</h1>
        <ul>
          <% skills.forEach(function(s) { %>
            <li>
              <a href="/skills/<%= s.id %>"><%= s.skill %></a>: <br>
                skill level - 
              <%= s.skillLevel %>
            </li> <br>
            <td>
              <form action="/skills/<%= s.id %>?_method=DELETE" method="POST">
                <button type="submit" class="btn btn-danger">X</button>
              </form>
            </td>
          <% }); %>
        </ul>
       
      </body>
</body>
</html>

<%- include('../partials/footer') %>